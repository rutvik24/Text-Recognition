                        android:fontFamily="@font/allerta"
                        android:text="Result"
                        android:id="@+id/Result"
                        android:layout_marginTop="5dp"
                        android:textStyle="bold"
                        android:textSize="25sp" />
                    <TextView
                        android:id="@+id/resultEt"
