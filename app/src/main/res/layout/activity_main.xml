                        android:textSize="25sp"
                        android:textStyle="bold" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/text_image"
                        android:layout_marginTop="15dp"
                        android:id="@+id/imageIv"/>

